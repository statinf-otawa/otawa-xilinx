<?xml version="1.0" encoding="UTF-8"?>
<cache-config>
	<!-- DOCUMENTATION
		https://fr.wikipedia.org/wiki/ARM_Cortex-A53
		https://www.7-cpu.com/cpu/Cortex-A53.html
		https://developer.arm.com/documentation/ddi0500/e/level-1-memory-system/about-the-l1-memory-system
		https://developer.arm.com/documentation/ddi0500/e/level-2-memory-system/about-the-l2-memory-system
	-->
	<dcache> <!-- 32 KB, 64 B/line, 4-WAY data cache -->
		<block_bits>8</block_bits>
		<set_bits>2</set_bits>
		<row_bits>5</row_bits>
		<next id="L2"> <!-- 1 MB 64 B/line, 16-way set-associative Level 2 cache -->
			<block_bits>11</block_bits>
			<row_bits>5</row_bits>
			<set_bits>4</set_bits>
		</next>
	</dcache>
	
	<icache> <!-- 32 KB, 64 B/line, 2-WAY instruction cache -->
		<block_bits>8</block_bits>
		<set_bits>1</set_bits>
		<row_bits>6</row_bits>
		<next ref="L2"/>
	</icache>
</cache-config>